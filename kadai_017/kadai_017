class Human:
    def __init__(self, name, age):
        self.name = name
        self.age = age
    
    def check_adult(self):
        if self.age < 20:
            print(f"{self.name}さんは大人でない")
        else:
            print(f"{self.name}さんは大人です")
        
yoshida = Human("吉田大", 22)
tanaka = Human("田中一", 11)
kuchiki = Human("朽木", 33)

human_list = [yoshida, tanaka, kuchiki]

for human in human_list:
    human.check_adult()
